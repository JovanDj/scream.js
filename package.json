		"dev": "run-p start vite",
		"lint": "biome check",
		"lint:fix": "biome check --write",
		"prepare": "husky",
		"start": "tsx --watch server.ts",
		"test": "cross-env NODE_ENV=test run-p test:integration test:e2e",
